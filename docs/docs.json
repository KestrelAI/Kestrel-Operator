  },
  "favicon": "/logo/light.svg",
  "colors": {
    "primary": "#3B82F6",
    "light": "#93C5FD",
    "dark": "#1E3A8A"
  },
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": [
          "introduction",
          "quickstart"
        ]
      },
      {
        "group": "Kubernetes",
        "pages": [
          "kubernetes/overview",
          "kubernetes/onboarding",
          "kubernetes/configuration",
          "kubernetes/troubleshooting"
        ]
      },
      {
        "group": "Cloud",
        "pages": [
          "cloud/overview",
          "cloud/aws",
          "cloud/oci"
        ]
      },
      {
        "group": "Integrations",
        "pages": [
          "integrations/slack",
          "integrations/cicd",
          "integrations/pagerduty",
          "integrations/tribal-knowledge"
        ]
      },
      {
        "group": "On-Premise Deployment",
        "pages": [
          "on-premise/overview",
          "on-premise/setup"
        ]
      }
    ],
    "anchors": [
      {
        "anchor": "Blog",
        "href": "https://usekestrel.ai/blog",
        "icon": "newspaper"
      },
      {
        "anchor": "Changelog",
        "href": "https://usekestrel.ai/changelog",
        "icon": "megaphone"
      }
    ]
