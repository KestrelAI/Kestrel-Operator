      "og:image": "https://usekestrel.ai/og-image.png"
    }
  },
  "redirects": [
    {
      "source": "/operator/overview",
      "destination": "/kubernetes/overview"
    },
    {
      "source": "/operator/onboarding",
      "destination": "/kubernetes/onboarding"
    },
    {
      "source": "/operator/configuration",
      "destination": "/kubernetes/configuration"
    },
    {
      "source": "/operator/troubleshooting",
      "destination": "/kubernetes/troubleshooting"
    }
